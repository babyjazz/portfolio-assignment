{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "endOfLine": "auto",
  "trailingComma": "all",
  "tabWidth": 2,
  "singleQuote": true,
  "semi": false,
  "importOrder": [
    "^react",
    "^.(css|scss)$",
    "<THIRD_PARTY_MODULES>",
    "^(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
